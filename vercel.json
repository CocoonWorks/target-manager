{
  "functions": {
    "app/api/targets/[id]/upload/route.ts": {
      "maxDuration": 60
    },
    "app/api/targets/[id]/upload/presigned/route.ts": {
      "maxDuration": 30
    },
    "app/api/targets/[id]/upload/confirm/route.ts": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/api/targets/(.*)/upload",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        }
      ]
    }
  ]
}
